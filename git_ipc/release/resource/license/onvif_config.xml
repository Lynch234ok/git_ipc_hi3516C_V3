		<scope>onvif://www.onvif.org/location/country/china</scope>
		<scope>onvif://www.onvif.org/type/video_encoder</scope>
		<scope>onvif://www.onvif.org/type/audio_encoder</scope>
		<scope>onvif://www.onvif.org/type/video_analytics</scope>
		<scope>onvif://www.onvif.org/type/Network_Video_Transmitter</scope>		
		<scope>onvif://www.onvif.org/hardware/IPC-model</scope>		
		<event>
			<renew_interval>60</renew_interval>	
			<simulate_enable>1</simulate_enable>
			<simulate_interval>10</simulate_interval>
		</event>		
